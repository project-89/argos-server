{
  "name": "functions",
  "version": "1.0.0",
  "main": "src/index.js",
  "dependencies": {
    "@google-cloud/firestore": "^7.1.0",
    "@types/express": "^4.17.3",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^11.0.0",
    "firebase-functions": "^6.1.1",
    "technicalindicators": "^3.1.0"
  },
  "engines": {
    "node": "22"
  },
  "scripts": {
    "lint": "eslint .",
    "serve": "firebase emulators:start --only functions,firestore,pubsub",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "npm install && firebase deploy --only functions",
    "logs": "firebase functions:log --project argos-434718",
    "logs:watch": "firebase functions:log -n 1000 --project argos-434718",
    "env:sync": "node scripts/setEnv.js",
    "env:list": "firebase functions:secrets:list --project argos-434718",
    "env:get": "firebase functions:secrets:get",
    "env:set": "firebase functions:secrets:set",
    "generate-key": "node scripts/generateApiKey.js",
    "realityStabilityTest": "node src/test/simulateStabilityIndex.js"
  },
  "devDependencies": {
    "eslint": "^9.15.0",
    "eslint-config-google": "^0.14.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0"
  }
}
