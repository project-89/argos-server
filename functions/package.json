{
  "name": "functions",
  "version": "1.0.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@google-cloud/firestore": "^7.1.0",
    "@types/express": "^4.17.3",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^11.11.1",
    "firebase-functions": "^6.1.1",
    "module-alias": "^2.2.3",
    "technicalindicators": "^3.1.0"
  },
  "engines": {
    "node": "18"
  },
  "scripts": {
    "build": "rimraf lib && tsc && tsc-alias",
    "build:watch": "tsc --watch",
    "lint": "eslint .",
    "serve": "npm run kill-ports && npm run build && firebase emulators:start --only functions,firestore,pubsub",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "kill-ports": "kill-port 5001 8080 8085 9229 5000 8081 9000 9090 9099 4000 4400 4500"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.29.0",
    "firebase-functions-test": "^3.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "kill-port": "^2.0.1",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.8"
  },
  "private": true
}
